{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/index.jsx",
      "use": "@vercel/react"
    },
    {
      "src": "jobs.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/jobs.json"
    }
  ]
}